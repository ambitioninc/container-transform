    {
        "acceptedResourceRoles": ["*"],
        "container": {
            "docker": {
                "forcePullImage": true,
                "image": "mysql",
                "network": "BRIDGE",
                "portMappings": [
                    {
                        "containerPort": 3306,
                        "hostPort": 0,
                        "protocol": "tcp"
                    }
                ]
            },
            "type": "DOCKER"
        },
        "env": {
            "MYSQL_DATABASE": "ghost",
            "MYSQL_PASSWORD": "password",
            "MYSQL_ROOT_PASSWORD": "root",
            "MYSQL_USER": "ghost"
        },
        "fetch": [],
        "id": "mysql",
        "requirePorts": false
    }
